
{
  "manifest_version": 3,
  "name": "PhishGuard",
  "version": "1.0",
  "description": "Your first line of defense against phishing attacks.",
  "permissions": ["activeTab", "tabs", "storage", "webNavigation"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "options_page": "options.html"
}
